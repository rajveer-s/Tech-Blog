<div class="row">

  <h3 id="welcomeText">(Welcome) => { {{name}} }</h3>

  <hr>

  <button type="submit" id="createpostbtn" class="btn btn-dark m-3"><a href="dashboard/makeanewpost">Make a new
      Post ().val</a></button>
</div>

{{#each posts as |p|}}
<div class="card-group">
  <div class="card">
    <div class="card-body">
      <a href="/dashboard/edit/{{id}}">
        <h5 class="card-title eachtitle" data-dashboard-text="Click here to edit or delete the post">{{p.title}}
          <span>{{format_time
            date_created}}</span>
        </h5>
      </a>
      <h5 class="card-text" id="pcontent">{{p.content}}</h5>
    </div>
  </div>
</div>
{{/each}}